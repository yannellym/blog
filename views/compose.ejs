<div class="form-group">
<form action="/compose" method="post">
    <label>Title</label>
    <input type="text" class="form-control" name="newComposeTitle" value="" placeholder="Title of post">
    <label>Body</label>
    <textarea class="form-control" name="newComposeBody" rows="3" placeholder="Write here"></textarea>
</div>

<button type="submit" class="btn btn-primary" value="">Publish</button> 
</form>

